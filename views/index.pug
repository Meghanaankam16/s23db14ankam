extends layout

block content
  h1= title
  p This is Meghana ankam.
  button(type="button", class="btn btn-danger", onclick="goFaster()") Press Me
  div#faster increase the speed.
  script.
    var mph = 20;
    var change = 1;

    function goFaster() {
      mph=mph+change;
      change=change+1;
      document.getElementById('faster').innerText = "MPH " + mph + " with change " + change;
    } 
